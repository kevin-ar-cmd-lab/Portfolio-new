    "/about": "org",
    "/contact": "org",
    "/blog": "blog",
    "/projects": "webpage",
    "/testimonials": "webpage",
  };

  const schemaType = routeSchemaMap[normalizedRoute] || "webpage";

  return (
    <>
      <SEOHead seo={seoData} schemaType={schemaType} />
      <FitToScreenLayout header={<Navbar />} footer={<Footer />}>
        {children}
      </FitToScreenLayout>
    </>
  );
}
